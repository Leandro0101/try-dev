{
  "name": "trydev",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/main/server.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "up": "docker-compose up --build",
    "down": "docker-compose down",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "migrations:run": "docker exec trydev_api yarn typeorm migration:run",
    "migrations:create": "docker exec trydev_api yarn typeorm migration:create -n",
    "migrations:revert": "docker exec trydev_api yarn typeorm migration:revert"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.22.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "aws-sdk": "^2.927.0",
    "bcrypt": "^5.0.1",
    "core-js": "^3.14.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}

{
  "name": "trydev",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "migrations:run": "sudo docker exec a22 yarn typeorm migration:run",
    "migrations:create": "sudo docker exec a22 yarn typeorm migration:create -n",
    "migrations:revert": "sudo docker exec a22 yarn typeorm migration:revert"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}

{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "CommonJS",
    "target": "es2019",
    "esModuleInterop": true,
    "allowJs": true,
    "experimentalDecorators": true,        
    "emitDecoratorMetadata": true,
    "baseUrl":"./",
    "paths": {
      "@/*": ["./*"],
      "@domain/*": ["src/domain/*"],
      "@infra/*": ["src/infra/*"],
    }
  }
}